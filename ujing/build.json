{
    "explorer": false,
    "requirejs": false,
    "build": true,
    "concat": true,
    "zip": true,
    "explorer-timeout": 1,
    "requirejs-timeout": 10000,
    "build-timeout": 60000,
    "concat-origin-timeout": 90000,
    "concat-timeout": 94000,
    "zip-timeout": 95000,
    "clean-after-zip": true,
    "clean-lib-before-build": false,
    "jscs": false,
    "jscs-default": false,
    "jshint": false,
    "jshint-failed": true,
    "jshint-default": false,
    "csslint": false,
    "csslint-default": false,
    "csslint-reporter": true,
    "minifycss": true,
    "autoprefixer": false,
    "js-uglify": false,
    "concat-js-uglify": true,
    "concat-configuration-properties": true,
    "use-tools-cfg-file": false,
    "default-index": "index.html",
    "default-config": "CubeModule.json",
    "package-path": "./",
    "config-file": "CubeModule.json",
    "karma": {
        "timeout": 3000,
        "path": "./",
        "conf": "karma.conf.js",
        "enabled": false,
        "enabledForTestEnvironment": true,
        "enabledForProductEnvironment": true, 
        "singleRun": false
    }
}
